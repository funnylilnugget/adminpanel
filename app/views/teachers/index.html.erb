<table>
<tr>
  <th>First Name</th>
  <th>Last Name</th>
  <th>Hero Name</th>
  <th>Quirk</th>
  <th>Age</th>
  <th>Salary</th>
  <th>Education</th>
  </tr>
  <% @teacher.each do |s| %>
    <tr>
      <td><%= s.first_name %></td>
      <td><%= s.last_name %></td>
      <td><%= s.hero_name %></td>
      <td><%= s.quirk %></td>
      <td><%= s.age %></td>
      <td><%= s.salary %></td>
      <td><%= s.education %></td>
      <td><a href="/teachers/<%=s.id%>">View Teacher</a></td>
    </tr>
    <% end %>
</table>
