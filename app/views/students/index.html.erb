<h2>All U.A. Students</h2>

<table class="table-shade" align="center">
  <tr>
  <th>First Name</th>
  <th>Last Name</th>
  <th>Hero Name</th>
  <th>Quirk</th>
  <th>Age</th>
  <th>Education</th>
  <th>View | Edit | Delete:</th>
  </tr>
<% @student.each do |s| %>
    <tr>
      <td><%= s.first_name %></td>
      <td><%= s.last_name %></td>
      <td><%= s.hero_name %></td>
      <td><%= s.quirk %></td>
      <td><center><%= s.age %></center></td>
      <td><%= s.education %></td>
      <td><center><a href="/students/<%=s.id%>">View Student</a></center></td>
    </tr>
    <% end %>
</table>
