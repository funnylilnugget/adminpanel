<h2>All U.A. Students</h2>

<table>
  <tr>
  <th>First Name</th>
  <th>Last Name</th>
  <th>Hero Name</th>
  <th>Quirk</th>
  <th>Age</th>
  <th>Education</th>
  </tr>
<% @student.each do |s| %>
    <tr>
      <td><%= s.first_name %></td>
      <td><%= s.last_name %></td>
      <td><%= s.hero_name %></td>
      <td><%= s.quirk %></td>
      <td><%= s.age %></td>
      <td><%= s.education %></td>
      <td><a href="/students/<%=s.id%>">View Student</a></td>
    </tr>
    <% end %>
</table>
